# Either rely on EC2 instance role or set these explicitly
AWS_REGION=ap-northeast-1
# AWS_ACCESS_KEY_ID=...
# AWS_SECRET_ACCESS_KEY=...
# AWS_SESSION_TOKEN=...

# Required: base64 (single-line) of your Lightsail private key for the region
LIGHTSAIL_PRIVATE_KEY_
LIGHTSAIL_KEYPAIR_NAME=lightsail-keypair-ap-northeast-1

# Optional overrides
AVAILABILITY_ZONE=ap-northeast-1a
CRON_SCHEDULE="0 */6 * * *"
S3_URI=s3://outline-link/sslink.txt
S3_BUCKET=outline-link
S3_KEY=sslink.txt
S3_ACL=public-read
TZ=UTC
